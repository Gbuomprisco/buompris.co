{
  "name": "jekyll-typescript-webpack",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "concurrently --color --raw 'npm run jekyll:watch' 'npm run webpack-dev-server'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "webpack --config config/webpack/webpack.prod.config.js",
    "webpack-dev-server": "webpack-dev-server --config config/webpack/webpack.dev.config.js",
    "jekyll:build": "bundle exec jekyll build",
    "jekyll:build:gh-pages": "bundle exec jekyll build --config _config.yml,config/jekyll/_config.gh-pages.yml",
    "jekyll:build:prod": "bundle exec jekyll build --config _config.yml,config/jekyll/_config.production.yml",
    "jekyll:watch": "bundle exec jekyll build --watch",
    "imagemin": "node ./scripts/imagemin",
    "htmlmin": "node ./scripts/htmlmin",
    "build": "node ./scripts/build",
    "build:gh-pages": "npm run jekyll:build:gh-pages && npm run webpack && npm run imagemin && npm run htmlmin",
    "build:prod": "npm run jekyll:build:prod && npm run webpack && npm run imagemin && npm run htmlmin",
    "deploy": "node ./scripts/deploy",
    "gh-pages": "node ./scripts/gh-pages"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.4.1",
    "@angular/compiler": "2.3.1",
    "@angular/compiler-cli": "2.4.1",
    "@angular/core": "2.4.1",
    "@angular/forms": "2.4.1",
    "@angular/platform-browser": "2.4.1",
    "@angular/platform-browser-dynamic": "2.4.1",
    "ng2-tag-input": "^0.7.9",
    "rxjs": "^5.0.2"
  },
  "devDependencies": {
    "@types/es6-collections": "^0.5.29",
    "@types/es6-promise": "0.0.32",
    "@types/es6-shim": "0.0.31",
    "@types/jasmine": "^2.2.34",
    "@types/node": "^6.0.39",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^6.5.1",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "concurrently": "^3.1.0",
    "core-js": "^2.4.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "gh-pages": "^0.12.0",
    "html-minifier": "^3.1.0",
